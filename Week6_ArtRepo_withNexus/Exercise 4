#clone Node app
git clone git@gitlab.com:twn-devops-bootcamp/latest/04-build-tools/node-app.git


sudo apt install -y npm

npm install
npm set registry  http://138.68.150.44:8081/repository/npm-hosted/
npm run build
npm run
npm publish --registry=http://138.68.150.44:8081/repository/npm-hosted/ nodejs-app-1.0.0.tgz 

npm publish --registry=http://138.68.150.44:8081/repository/npm-hosted/ node
npm adduser --auth-type=legacy --registry=http://138.68.150.44:8081/repository/npm-hosted/
